{
  "name": "akkad-example-app",
  "version": "0.0.2",
  "description": "An Akkad example application",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js --display-error-details",
    "build": "npm run rmrf && npm install && npm run build:webpack",
    "start:dev": "node devServer.js",
    "start:prod": "node prodServer.js",
    "start": "npm run build && npm run start:prod",
    "lint": "eslint src",
    "rmrf": "rm -rf ./node_modules",
    "setup:dev": "npm run rmrf && npm link akkad && npm install && npm install && npm install react && npm install react-dom && npm start:dev"
  },
  "engine": {
    "node": "5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brochington/Akkad"
  },
  "author": "Broch Stilley <brochington@gmail.com> (http://github.com/brochington)",
  "bugs": {
    "url": "https://github.com/brochington/Akkad/issues"
  },
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "express": "^4.13.3",
    "file-loader": "^0.8.4",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.0",
    "rimraf": "^2.4.3",
    "source-map-loader": "^0.1.5",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.0.0"
  },
  "dependencies": {
    "akkad": "^1.1.0",
    "babylonjs": "^2.2.0",
    "history": "^1.12.5",
    "immutable": "^3.7.5",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-router": "^1.0.0-rc3"
  }
}
